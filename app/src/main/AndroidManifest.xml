		</activity>
		<activity
			android:label = "@string/title_activity_sub_activity"
			android:name = ".windowanimations.SubActivity"
			>
		</activity>
		<activity
			android:label = "@string/title_activity_animated_sub"
			android:name = ".windowanimations.AnimatedSubActivity"
			>
		</activity>
		<activity
			android:label = "@string/title_activity_window_animations"
			android:name = ".windowanimations.WindowAnimations"
			>
		</activity>
		<activity
			android:label = "@string/title_activity_layout_trans"
			android:name = ".layouttranschanging.LayoutTrans"
			>
		</activity>
		<activity
			android:label = "@string/title_activity_interpolator"
			android:name = ".interpolator.InterpolatorActivity"
			>

		</activity>
		<activity
			android:label = "@string/title_activity_transition"
			android:name = ".transition.TransitionActivity"
			>
			<intent-filter>
				<action android:name = "android.intent.action.MAIN"/>

				<category android:name = "android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
	</application>

</manifest>
